import java.util.*;

public class aq3 {
    public static void main(String args[]){
        int n;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a number ");
        n=sc.nextInt();
        for(int i=1;i<11;i++){
            int r=n*i;
            System.out.println(n+"x"+i+"="+r);
        }
    }
}
